<div aria-hidden="true" aria-labelledby="order-modal" role="dialog" tabindex="-1" id="edit-user-modal" class="modal fade" >
    <div id="Modal-dialog" class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button aria-hidden="true" data-dismiss="modal" class="close" id="closeFormEditUser" type="button">Ã—</button>
                <h4 class="modal-title"><%= json.user.editUser %></h4>
            </div>
            
            <form class="form-horizontal tasi-form" id="formEditUser" method="get" action="">
            
            <div class="modal-body">
 
                <div class="row">
                    <div class="col-lg-12">
                        <section class="panel">
                            <div class="panel-body">
                                
                                    <div class="form-group">
                                        <label for="firstName" class="col-lg-4 col-sm-4 control-label"><%= json.user.firstName %></label>
                                        <div class="col-lg-8">
                                            <input type="text" class="form-control" name="firstName" id="firstName" placeholder="" value="<%= user.firstName %>">
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label for="lastName" class="col-lg-4 col-sm-4 control-label"><%= json.user.lastName %></label>
                                        <div class="col-lg-8">
                                            <input type="text" class="form-control" name="lastName" id="lastName" placeholder="" value="<%= user.lastName %>">
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label for="email" class="col-lg-4 col-sm-4 control-label"><%= json.user.email %></label>
                                        <div class="col-lg-8">
                                            <input type="email" class="form-control" name="email" id="email" placeholder="" value="<%= user.email %>">
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label for="username" class="col-lg-4 col-sm-4 control-label"><%= json.user.username %></label>
                                        <div class="col-lg-8">
                                            <input type="text" class="form-control" name="username" id="username" placeholder="" value="<%= user.username %>">
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label for="department" class="col-lg-4 col-sm-4 control-label"><%= json.user.department %></label>
                                        <div class="col-lg-8">
                                            <input type="text" class="form-control" name="department" id="department" placeholder="" value="<%= user.department %>">
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label for="department" class="col-lg-4 col-sm-4 control-label"><%= json.user.timezone %></label>
                                        <div class="col-lg-8 timezone"></div>
                                    </div>
                            </div>
                        </section>
                    </div>
                </div>
                
           	</div>
        	<div class="modal-footer">
        		<button id="closeModal" type="button" class="btn btn-default bb-btn" data-dismiss="modal"><%= json.button.cancel %></button>
        		<button id="saveEditUser" type="submit" class="btn btn-default bb-btn"><%= json.button.save %></button>
      		</div>
      		
      		</form>
        </div>
    </div>
</div>