<div class="col-lg-3 act_row">
    <section class="panel">
        <div class="panel-body">
        <div class="col-lg-12">
            <img src="<%=imgURI%>">
        </div>
        <div class="col-lg-12 act_des">
                <label class=""><%= description %></label>
        </div>
        <div class="col-lg-12">
            <div class='md-checkbox'>
                <% if(haveOrders){%>
                    <input id='md-checkbox-<%= id_activity %>' class='md-check season-check' type="checkbox" <%= checked %> disabled>
                <% }else{%>
                    <input id='md-checkbox-<%= id_activity %>' class='md-check season-check' type="checkbox" <%= checked %>>
                <% }%>
                <label for="md-checkbox-<%= id_activity %>">
                    <span></span>
                    <span class="check"></span>
                    <span class="box"></span>
                </label>
            </div>
        </div>
        </div>
    </section>
</div>