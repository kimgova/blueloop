<div aria-hidden="true" aria-labelledby="myModalLabel" role="dialog" tabindex="-1" id="editTeamMember" class="modal fade" >
	<div class="modal-dialog">
		<div class="modal-content">
			<div class="modal-header">
				<button id="closeHeaderEditTeamMember" aria-hidden="true" data-dismiss="modal" class="close" type="button">Ã—</button>
				<h4 class="modal-title"><%= json.teamwork.addUsers %></h4>
			</div>
			<div class="modal-body">
				<div class="row">
					<div class="col-lg-12">
						<div class="panel-body">
							<div class="form"> 
								<form action="" method="" class="cmxform form-horizontal">
									<div class="form-group">
										<label class="col-lg-3 control-label"><%= json.general.users %></label>
										<div class="col-lg-8 form-inline">
											<div>
												<div id="member-list">
													<div id="member-container" style="float: left;">
                                                        <select multiple="multiple" id="member-select">
                                                            <% $.each(users, function(i, user){ %>
                                                                <option value="<%= user.id %>"><%= user.firstName + ' ' + user.lastName %></option>
                                                            <% }); %>
                                                        </select>
                                                    </div>
													<a href="" type="button" class="btn" id="lnkSelectMemb">
														<img title="<%= json.teamwork.inviteMember %>" class="ico_" src="/blueloop/static/images/select_member.jpeg" style="width: 25px; height: 25px;" />
													</a>
												</div>
											</div>
										</div>
									</div>

									<div class="form-group">									
										<div class="checkboxes">
											<label class="col-lg-3 control-label"><%= json.button.sendEmail %></label>											
											<label class="label_check c_on col-lg-6 " for="checkbox_sendEmail">
												<input id="checkbox_sendEmail" type="checkbox" value="1" name="checkbox_sendEmail">
												<%= json.notification.sendEmailNotification %>
											</label>
										</div>
									</div>
								</form>
							</div>
						</div>
					</div>
				</div>
			</div>
			<div class="modal-footer">
				<button id="closeEditTeamMember" type="button" class="btn btn-default" data-dismiss="modal"><%= json.button.close %></button>
				<button id="ShareMember" type="button" data-dismiss="modal" class="btn btn-primary"><%= json.button.add %></button>
			</div>
		</div>
	</div>
</div>