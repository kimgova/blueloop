<tr>
    <td colspan="4">
        <div>
            <form id="" class="cmxform form-horizontal">
                <div class="form-group">
                    <div class="col-lg-12">
                        <select id='selectNewUserRole' class="form-control search-input">
                        	<% $.each(userList, function(k, us){ %>
                            	<option value="<%= us.id %>"><%= us.firstName %> <%= us.lastName %></option>
                        	<% }); %>                        
                        </select>
                    </div>
                    <label class="error" for="selectNewUserRole" style="display:none">This field is required.</label>
                </div>
            </form>
        </div>
    </td>
   
    <td>
        <div>
            <button id='saveUserRole' class='btn btn-primary btn-xs'>
                <i class='fa fa-floppy-o'></i>
            </button>
            <button id='cancelUserRole' class='btn btn-danger btn-xs'>
                <i class='fa fa-times'></i>
            </button>
        </div>
    </td>
</tr>