<div class="col-lg-12">
	<label class="col-lg-12 col-sm-12 control-label"><%=json.forecast.skuPlanning.associateTitle%></label>
	<div class="col-lg-12">
		<table cellpadding='0' cellspacing='0' border='0'
			class='table table-striped table-hover table-bordered'
			id='tableAssoSku' style='text-align: center'>
			<thead>
				<tr><th class='center' style="display: none;">id</th>
					<th class='center'><%=json.forecast.skuPlanning.associateSku%></th>
					<th class='center'><%=json.forecast.skuPlanning.associateDes%></th>
					<th class='center'><%=json.forecast.skuPlanning.skuType%></th>
					<th class='center'><%= json.general.action %></th>
				</tr>
			</thead>
			<tbody id="tbodyAssoSku">
				<tr id="noData">
					<td id="no-data-td" valign="top" colspan="4">No data available
						in table</td>
				</tr>
			</tbody>
		</table>
	</div>
	<div class="col-lg-2">
		<button id="btnAssoSku" class="btn btn-primary btn-sm" type="button">
			<i class="fa fa-plus"></i> <%= json.general.addNew %>
		</button>
	</div>
</div>