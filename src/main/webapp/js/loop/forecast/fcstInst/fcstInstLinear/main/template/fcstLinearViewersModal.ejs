<div aria-hidden="true" aria-labelledby="myModalLabel" role="dialog" tabindex="-1" id="viewers-planning-modal" class="modal fade" >
	<div class="modal-dialog">
		 <div class="modal-content">
			 <div class="modal-header">
             	<button id="closeHeader" aria-hidden="true" data-dismiss="modal" class="close" type="button">Ã—</button>
             	<h4 class="modal-title"><%= 'Viewers' %></h4>
			</div>
			<div class="modal-body">
            	<div class="col-lg-12">
				    <table cellpadding='0' cellspacing='0' border='0' class='table table-striped table-hover table-bordered' id='tableForecastLinear' style='text-align: center'>
				        <thead>
				            <tr>
				                <th class='center'>Name</th>
				                <th class='center'>Last modification</th>
				            </tr>
				        </thead>
				        <tbody>
				           
				            <% if(data.length == 0){ %>
					            <tr>
					                <td id="no-data-td" valign="top" colspan="2">No data available in table</td>
					            </tr>
				            <% } else { %>
				           		<% $.each(data, function(i, us){ %> 
				           			<tr>
									    <td><div><%= us.name %></div></td>
									    <td><div><%= us.lastModification %></div></td>
									</tr>				           			
				                <% }); %>
				            <% } %>			                
				                
				        </tbody>
				    </table>
				</div>               
                  
           	</div>
        	<div class="modal-footer">
        		<button id="closeModal" type="button" class="btn btn-default" data-dismiss="modal"><%= json.button.close %></button>
      		</div>
		 </div>
	</div>
</div>