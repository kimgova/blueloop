<table cellpadding='0' cellspacing='0' border='0' class='table table-striped table-hover table-bordered' id='tableActWU' style='text-align: center'>
    <thead>
        <tr>
            <th class='center'><%= json.general.name %></th>
            <th class='center'><%= json.general.image %></th>
            <th class='center'><%= json.general.action %></th>
        </tr>
    </thead>
    <tbody>
        <tr id="no-data-tr">
            <td valign="top" colspan="3">No data available in table</td>
        </tr>
    </tbody>
</table>
<div style="float:right;">
    <button id="addActBtn" type="button" class="btn btn-primary btn-sm">
        <i class="fa fa-plus"></i> <%= json.general.addNew %>
    </button>
</div>