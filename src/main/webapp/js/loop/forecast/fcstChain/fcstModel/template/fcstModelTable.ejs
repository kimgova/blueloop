<table cellpadding='0' cellspacing='0' border='0' class='table table-striped table-hover table-bordered' id='tableForecastModel' style='text-align: center'>
    <thead>
        <tr>
            <th style='width:20%' class='center'><%= json.forecast.forecastModelName %></th>
            <th class='center'><%= json.forecast.forecastModelDescription %></th>
            <% if(forecastPermission == true){ %>
                <th class='center'><%= json.default.nameLabel %></th>
                <th class='center'><%= json.general.action %></th>
             <% } %>
        </tr>
    </thead>
    <tbody>
        <tr>
            <td valign="top" colspan="3">No data available in table</td>
        </tr>
    </tbody>
</table>