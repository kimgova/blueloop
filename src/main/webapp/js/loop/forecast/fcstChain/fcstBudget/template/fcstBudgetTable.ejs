<div>
    <table cellpadding='0' cellspacing='0' border='0' class='table table-striped table-hover table-bordered' id='tableForecastWU' style='text-align: center; width:80%; margin-left: 10%;'>
        <thead>
            <tr>
                <th class='center'><%= json.forecast.budget.ipoName %></th>
                <th class='center'><%= json.forecast.budget.uploadDate %></th>
                <th class='center'><%= json.general.action %></th>
            </tr>
        </thead>
        <tbody>
            <tr>
                <td id="no-data-td" valign="top" colspan="3"><%= json.forecast.noData %></td>
            </tr>
        </tbody>
    </table>    
</div>
<div id="btnsAction">
    <button id='uploadBudget' class='btn btn-primary' title="" data-placement="top" data-toggle="tooltip"
            data-original-title="<%= json.forecast.tooltips.uploadBudget %>" style='float:right;margin-right: 10%;'>
        <i class='fa fa-upload'></i>
        <%= json.forecast.budget.newbtn %>
    </button>
</div>