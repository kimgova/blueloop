<div>
    <table cellpadding='0' cellspacing='0' border='0' class='table table-striped table-hover table-bordered' id='tableForecastWU' style='text-align: center'>
        <thead>
            <tr>
                <th class='center'><%= json.forecast.forecastWU %></th>
                <th class='center'><%= json.forecast.forecastWUActivities %></th>
                <th class='center'><%= json.forecast.forecastWUParticipants %></th>
                <th class='center'><%= json.general.action %></th>
            </tr>
        </thead>
        <tbody>
            <tr id="no-data-tr">
                <td valign="top" colspan="4">No data available in table</td>
            </tr>
        </tbody>
    </table>
    <div style="float:right;">
        <button id="addWUBtn" type="button" class="btn btn-primary btn-sm">
            <i class="fa fa-plus"></i> <%= json.forecast.addWU %>
        </button>
        <button id="defineSequenceBtn" type="button" class="btn btn-primary btn-sm">
            <i class="fa fa-sort-amount-desc"></i> <%= json.forecast.defineSeq %>
        </button>
    </div>
</div>