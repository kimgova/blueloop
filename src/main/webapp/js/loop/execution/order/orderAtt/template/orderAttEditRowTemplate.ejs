<tr>
    <td><div><%= fileName %></div></td>
    <td>
        <div>
            <div class="form-group">
                <div class="col-lg-12">
                    <input id="fileDescription" name="fileDescription" type="text" class="form-control small focusedInput" value="<%= description %>">
                </div>
            </div>
        </div>
    </td>
    <td><div><%= uploader %></div></td>
    <td><div><%= creationDate %></div></td>
    <td>
        <div>
            <button id='saveEditAttach' class='btn btn-primary btn-xs'>
                <i class='fa fa-floppy-o'></i>
            </button>
            <button id='cancelEditAttach' class='btn btn-danger btn-xs'>
                <i class='fa fa-times'></i>
            </button>
        </div>
    </td>
</tr>