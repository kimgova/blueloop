<div aria-hidden="true" aria-labelledby="order-modal" role="dialog" tabindex="-1" id="new-order-modal" class="modal fade" >
    <div id ="myModal-dialog" class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button aria-hidden="true" data-dismiss="modal" class="close" id="closeFormNewOrder" type="button">Ã—</button>
                <h4 class="modal-title"><%= json.order.newOrder %></h4>
            </div>
            
            <form class="form-horizontal tasi-form" id="formNewOrder" method="get" action="">
            
            <div class="modal-body">
 
                <div class="row">
                    <div class="col-lg-12">
                        <section class="panel">
                            <div class="panel-body">
                                
                                    <div class="form-group">
                                        <label for="orderId" class="control-label col-lg-4" title="<%= json.order.orderId %>"><span class="badge bg-info">1</span> <%= json.order.addThe %> <%= json.order.orderId %>:</label>
                                        <br><br>
                                        <div class="row">
                                            <div class="col-lg-6 col-sm-6" style='margin-left: 15px;'>
                                                <input name="orderId" id="orderId" type="text" value='<%= orderNumber %>' <%= disabled %> class="form-control" required>
                                             </div>
                                        </div>
                                    </div>
                                    
                                    <div class="form-group">
                                        <label for="orderDta" class="control-label col-lg-4" title="<%= json.order.dtaHeader %>" ><span class="badge bg-info">2</span> <%= json.order.addThe %> <%= json.order.dtaHeader %>:</label>
                                        <br><br>
                                        <div class="row">
                                            <div class="col-lg-6 col-sm-6">
                                                <div data-date-viewmode="years" data-date-format="dd-mm-yyyy" class="input-append date dpYears" style='margin-left: 15px;'>
                                                    <input type="text" readonly="" size="16"  value='<%= dta %>' class="form-control" id="orderDta" name="orderDta" required>
                                                    <span class="input-group-btn add-on">
                                                        <button class="btn btn-danger" type="button"><i class="fa fa-calendar"></i></button>
                                                    </span>
                                                </div>
                                             </div>
                                        </div>
                                    </div>
                                                       
                                    <div class="form-group">
                                        <label for="orderSkus" class="control-label col-lg-4" title="<%= json.order.skus %>"><span class="badge bg-info">3</span> <%= json.order.addThe %> <%= json.order.skus %>:</label>
                                        <br><br>
                                        <div class="row">
                                            <div class="col-lg-12">
                                                <div class='clearfix'>
                                                    <div class='btn-group' style='margin-left: 15px;'>
                                                        <button id='addOderSku' class='btn green'><%= json.general.addNew %> <i class='fa fa-plus'></i></button>
                                                    </div>
                                                </div>
                                                <br>
                                                <div class='' style='margin-left: 15px;'>
                                                    <label id='skusError' style='display:none; color: red;'><%= json.order.errorSKUs %></label>
                                                </div>
                                                <div id="addedSkusTable"></div>
                                            </div>
                                        </div>
                                    </div>
                                    
                                     <div class="form-group">
                                        <label for="" class="control-label col-lg-4"><b><%= json.order.observations %>:</b></label>
                                        <br><br>
                                        <div class="row">
                                            <label class="control-label col-lg-4" id='observationPTA' style='display:none;margin-left: 15px;'><%= json.order.observation1 %></label>
                                        </div>
                                    </div>
                                    
                            </div>
                        </section>
                    </div>
                </div>
                
           	</div>
        	<div class="modal-footer">
        		<button id="closeModal" type="button" class="btn btn-default" data-dismiss="modal"><%= json.button.close %></button>
        		<button id="saveNewOrder" type="submit" class="btn btn-primary"><%= json.button.save %></button>
      		</div>
      		
      		</form>
        </div>
    </div>
</div>